{{- $pages := where .Site.RegularPages "Section" "post" -}}
{{- $talks := where $pages "Params.categories" "talk" -}}
{{- $latest := index $talks 0 -}}
{{- if $latest -}}
    {{- $number := $latest.Title | findRE "#([0-9]+)" -}}
    {{- if $number -}}
        {{- (index $number 0) | replaceRE "#" "" | safeHTML -}}
    {{- end -}}
{{- end -}}
